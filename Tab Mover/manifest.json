{
  "manifest_version": 3,
  "name": "Tab Mover",
  "version": "1.0",
  "description": "Move tabs from one window to another.",
  "permissions": [
    "tabs",
    "system.display"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  }
}
